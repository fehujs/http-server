    "@fehujs/tsconfig": "^1.0.0",
    "@types/formidable": "^3.4.5",
    "@types/node": "^22.10.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@fehujs/ioc": "^1.0.2",
    "@fehujs/template-parser": "^1.0.0",
